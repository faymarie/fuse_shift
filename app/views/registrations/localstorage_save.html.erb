<script>
  $( document ).ready(function() {
    let encrypted_data = <%= @encrypted_data.to_json.html_safe %>;
    let hashed_email = <%= @hashed_email.to_json.html_safe %>;
    localStorage.setItem(hashed_email, JSON.stringify(encrypted_data));
    window.location.replace('/registrations/'+hashed_email);
  });
</script>