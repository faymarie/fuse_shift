<h1><%= t("views.new.headline")%></h1>
<p><%=t("views.new.above_form")%></p>    

<input type="text" id="flatpickr-input">

<script type="text/javascript">
  $(document).ready(function(){
    flatpickr('#flatpickr-input', {
      enableTime: true,
      dateFormat: "Y-m-d H:i",
      minDate: "2018-10-13 12:00",
      maxDate: "2018-10-21 12:00"
    });
  });
</script>

<%= form_for(@registration, html: {class: 'form-horizontal'}) do |f| %>
  <%= render 'form', :f => f %>
<% end %>          

  


  

